                    </dependencies>
                    <control format="string" type="spinner"/>
                </setting>
            </group>
            <group id="4">
                <setting id="kodion.install.inputstreamhelper" type="action" label="30623" help="">
                    <level>0</level>
                    <constraints>
                        <allowempty>true</allowempty>
